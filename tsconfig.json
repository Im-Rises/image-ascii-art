{
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "declaration": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "strictNullChecks": true,
    "noImplicitAny": true
    //    "baseUrl": ""
  },
  "include": [
    "src"
  ]
}
